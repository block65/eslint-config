{
  "env": {},
  "globals": {},
  "parser": "/home/mholman/Projects/block65/eslint-config/node_modules/@typescript-eslint/parser/dist/index.js",
  "parserOptions": {
    "tsconfigRootDir": "/home/mholman/Projects/block65/eslint-config/packages/test/__test__/fixtures/typescript",
    "project": [
      "./tsconfig.json"
    ],
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "plugins": [
    "import",
    "@typescript-eslint"
  ],
  "rules": {
    "import/no-unresolved": [
      "error"
    ],
    "import/extensions": [
      0
    ],
    "import/order": [
      "error",
      {
        "newlines-between": "always-and-inside-groups",
        "warnOnUnassignedImports": false
      }
    ],
    "import/named": [
      "off"
    ],
    "import/namespace": [
      "error"
    ],
    "import/default": [
      "error"
    ],
    "import/export": [
      "error"
    ],
    "import/no-named-as-default": [
      "warn"
    ],
    "import/no-named-as-default-member": [
      "warn"
    ],
    "import/no-duplicates": [
      "warn"
    ]
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [
        ".ts",
        ".tsx"
      ]
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true,
        "project": [
          "./tsconfig.json"
        ]
      },
      "node": {
        "extensions": [
          ".ts",
          ".tsx",
          ".js",
          ".jsx"
        ]
      }
    },
    "import/extensions": [
      ".ts",
      ".tsx",
      ".js",
      ".jsx"
    ],
    "import/external-module-folders": [
      "node_modules",
      "node_modules/@types"
    ]
  },
  "ignorePatterns": []
}
